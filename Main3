import java.util.ArrayList;

public class DZ2 {

    static double average (ArrayList<Integer> list) {

        ArrayList<Integer> l = new ArrayList<Integer>();

        list.add(5);
        list.add(10);

        System.out.println(average(list));

        double sum = 0;

        for(int i=0;i<list.size();i++) {
            sum+=list.get(i);
        }

        return sum/list.size();

               

    }

}
